# Google Sheets API Configuration
# Copy this file to .env and replace with your actual values

# Your Google Sheets API Key (get from https://console.developers.google.com/)
REACT_APP_GOOGLE_SHEETS_API_KEY=your_api_key_here

# Your Google Spreadsheet ID (the long string in your Google Sheets URL)
REACT_APP_GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here

# Optional: Custom sheet names (defaults provided if not set)
REACT_APP_GOOGLE_SHEETS_PLAYERS_SHEET=Players
REACT_APP_GOOGLE_SHEETS_MATCHDAYS_SHEET=Matchdays
REACT_APP_GOOGLE_SHEETS_STATISTICS_SHEET=Statistics
